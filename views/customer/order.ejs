    <div class="order-details-cont">
        <div class="order-process">
            <ul>
                <li>
                    <img src="https://img.icons8.com/external-flatart-icons-flat-flatarticons/64/000000/external-box-design-thinking-and-3d-model-printing-flatart-icons-flat-flatarticons.png"/><br>
                    <ion-icon class="order-icon" name="checkmark-outline"></ion-icon>
                    <p>Processing</p>
                </li>
                <li>
                    <img src="https://img.icons8.com/external-vitaliy-gorbachev-lineal-color-vitaly-gorbachev/60/000000/external-drone-delivery-stay-home-vitaliy-gorbachev-lineal-color-vitaly-gorbachev.png"/><br>
                    <ion-icon class="order-icon" name="sync-outline"></ion-icon>
                    <p>Shipping</p>
                </li>
                <li>
                    <img src="https://img.icons8.com/fluency/48/000000/delivered-box.png"><br>
                    <ion-icon class="order-icon" name="close-outline"></ion-icon>
                    <p>Delivered</p>
                </li>
            </ul>
        </div>
        <div class="order-info-cont">
            <p class="order-num">Order no. 12345</p>
            <p class="order-placed-date">Placed on: 01 Jun 2021</p>
            <p class="order-delivered-date">Delivered on:</p>
        </div>
        <hr>
        <div class="order-client-detail">
            <div class="row client-detail-name">
                <p class="static-text">Customer Name</p>
                <p>Marco Sardido</p>
            </div>
            <div class="row client-detail-address">
                <p class="static-text">Customer Address</p>
                <p>2168 Hilltop Haven Drive, Los Angeles, California</p>
                <p>90017</p>
            </div>
            <div class="row client-detail-contact">
                <p class="static-text">Customer Number</p>
                <p>+1 310-776-3966</p>
            </div>
        </div>
        <div class="order-client-items">
            <div class="item-row">
                <div class="item-img">
                    <img src="/public/assets/images/iphone-7-black.jpg" alt="IPhone 7">
                </div>
                <div class="item-desc">
                    <p class="item-title">IPhone 7 Pro Limited Black Edition</p>
                    <p class="item-color">Color: Black</p>
                    <p class="item-warranty">Warranty: 24 Months</p>
                </div>
                <p>Qty: 2</p>
                <p>₱40,000.00</p>
            </div>
        </div>
        <hr>
        <div class="order-client-total">
            <p class="temp">Total Summary</p>
            <div class="total-summary">
                <div class="static total-static">
                    <p>Subtotal(2 Item/s)</p>
                    <p>Shipping Fee</p>
                </div>
                <div class="total-dynamic">
                    <p>₱40,000.00</p>
                    <p>₱42.00</p>
                </div>
            </div>
            <hr>
            <div class="total">
                <div class="static total-static">
                    <p>Total(VAT Incl.)</p>
                </div>
                <div class="total-dynamic">
                    <p>₱40,042.00</p>
                </div>
            </div>
            <p class="temp">Paid by Cash on Delivery</p>
        </div>
    </div>
</div>
