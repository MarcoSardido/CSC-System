<link rel="stylesheet" href="/public/assets/css/customer/order.css">


<div class="order-header">
    <div class="icon">
        <div class="notif"></div>
        <ion-icon name="notifications"></ion-icon>
    </div>
</div>
<div class="order-body">
    <div class="order-list-cont">
        <div class="title-cont">
            <p>My Orders</p>
        </div>
        <div class="tabs-cont">
            <a href="#" class="order-tab-link active">All</a>
            <a href="#" class="order-tab-link">To Ship</a>
            <a href="#" class="order-tab-link">To Receive</a>
            <a href="#" class="order-tab-link">Delivered</a>
        </div>
        <div id="order-filter-all" style="display: block">
            <div class="list-cont">
                <div class="list-dynamic-cont">
                    <div class="list-row-order active">
                        <div class="left-desc">
                            <p class="list-item-store">Apple</p>
                            <p class="list-item-status-ship">Shipped</p>
                        </div>
                        <div class="right-desc">
                            <p class="list-item-price">₱40,042.00</p>
                        </div>
                    </div>
                </div>
                <div class="list-dynamic-cont">
                    <div class="list-row-order">
                        <div class="left-desc">
                            <p class="list-item-store">Shein</p>
                            <p class="list-item-status-process">Processing</p>
                        </div>
                        <div class="right-desc">
                            <p class="list-item-price">₱342.00</p>
