    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>City Sales Cloud</title>
    <!-- Meta properties -->
    <meta property="og:title" content="City Sales Cloud">
    <meta property="og:description" content="Online Live Selling Platform in the Philippines">
    <meta property="og:type" content="website">
    <meta name="keywords" content="Online Live Selling, Live Selling Platform, Online Selling Philippines">

    <!-- Icon -->
