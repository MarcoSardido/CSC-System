    <script defer src="https://muazkhan.com:9001/socket.io/socket.io.js"></script>
    
    <!-- Stripe -->
    <script defer src="https://js.stripe.com/v3/"></script>

    <!-- Charts -->
    <script defer src="https://cdn.jsdelivr.net/npm/chart.js@3.5.1/dist/chart.min.js"></script>

    <!-- My Script -->
    <script defer src="/public/script/general.js"></script>
    <!-- <script defer src="/public/script/customer.js"></script> -->

    <!-- General Style -->
    <link rel="stylesheet" href="/public/assets/css/customer/general.css">

    <!-- Sidebar Style -->
    <% if (onLive === false) { %>
        <link rel="stylesheet" href="/public/assets/css/customer/sidebar.css">
    <% } %>
   
