<div class="right">
    <div class="top">
        <button id="menu-btn">
            <ion-icon name="menu-sharp" style="font-size: 2rem;"></ion-icon>
        </button>
        <div class="theme-toggler">
            <span class="active">
                <ion-icon name="sunny-sharp"></ion-icon>
            </span>
            <span>
                <ion-icon name="moon-sharp"></ion-icon>
            </span>
        </div>
        
        <% displayAccountInfo.forEach(accountKey=> { %>
            <div class="profile">
                <div class="info">
                    <p>Hey, <b><%= accountKey.displayName === '' ? 'Customer' : accountKey.displayName %></b></p>
                    <small class="text-muted"><%= accountKey.isVerified ? 'Verified 👍' : 'Not Verified 👎' %></small>
                    
                </div>
                <div class="profile-photo">
                    <% if(accountKey.userPhoto==='' ) { %>
                        <img src="/public/assets/images/default/user-default-profile.png" alt="<%=accountKey.displayName%>">
                        <% } else { %>
                            <img src="data:<%=accountKey.imgType%>;base64,<%=accountKey.userPhoto%>"alt="<%=accountKey.displayName%>">
                    <% } %>
                </div>
            </div>
        <% }); %>
    </div>
    <!-- END OF TOP -->

